if $ARGS[0] = 'food':
	if money < 100:
		'You do not have enough money for the cafe.'
		act 'Leave':gt 'park', 'start'
		exit
	end

	cla
	health += 10
	money -= 100
	manna += 5
	willpower += 5

	if energy >= 30:
		fat += 5
