# party_boy
if $ARGS[0] = 'party_boy':
	cls
	minut += 5
	party_dj = 1
	gs'stat'
	if party_dj = 1 and dj_beer < 1:
		'<center><b><font color = maroon>DJ</font></b></center>'
		'<center><img src="images/qwest/alter/sisterQW/ReksHome/Party_boy.jpg"></center>'
		'The guy that-there configures and makes the sound even louder.
		- Hello, <<$name>>, get me a beer, in the kitchen all the booze they find.'
	elseif dj_beer = 1 and dj_thank < 1:
		'<center><b><font color = maroon>DJ</font></b></center>'
		'<center><img src="images/qwest/alter/sisterQW/ReksHome/Party_boy.jpg"></center>'
		'The guy that-there configures and makes the sound even louder.
		-Well, <<$name>>, where our beer.
		You have in the kitchen and two bottles of beer can <a href="exec:dynamic $party_dj_beer">transmit</a> their guys.'
	elseif dj_thank = 1 and dj_bad < 1:
		'<center><b><font color = maroon>DJ</font></b></center>'
		'<center><img src="images/qwest/alter/sisterQW/ReksHome/Party_boy.jpg"></center>'
		'The guy that-there configures and makes the sound even louder.
		you are allowed <a href="exec:dynamic $party_dj_thank">twist</a> turntable.'
	elseif dj_bad = 1 and dj_boobs < 1:
		'<center><b><font color = maroon>DJ</font></b></center>'
		'<center><img src="images/qwest/alter/sisterQW/ReksHome/Party_boy.jpg"></center>'
		'-<<$name>>, but you also do not get a damn, Do not even try this time.'
	elseif dj_boobs = 1:
		'<center><b><font color = maroon>DJ</font></b></center>'
		'<center><img src="images/qwest/alter/sisterQW/ReksHome/Party_boy.jpg"></center>'
		'The guy that-there configures and makes the sound even louder.
		You again allowed <a href="exec:dynamic $party_dj_cock">twist</a> turntable.'
	end
	act 'Move away':gt $locM,$metkaM
	if dj_bad = 1 and alko >=3: act 'bare chest':dynamic $party_dj_boobs
end

