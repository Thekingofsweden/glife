# cum_cleanup

cumsptmp = 0

:cumsptmploop
arrmodtmp = arrsize('sparrvol')
if arrmodtmp < (cumsptmp+1): cumspclnt = 0

!{Full Body Clean}
if cumspclnt = 1:
	if sparrloc = 0 or sparrloc = 1 or sparrloc = 3 or sparrloc = 4 or (sparrloc >= 8 and sparrloc < 12) or sparrloc > 12:
		KILLVAR 'spafinloc',cumsptmp
		KILLVAR 'sparrage',cumsptmp
		KILLVAR 'sparrloc',cumsptmp
		KILLVAR '$sparrnam',cumsptmp
		KILLVAR 'sparrppt',cumsptmp
		KILLVAR 'sparridt',cumsptmp
		KILLVAR 'sparrvol',cumsptmp
		KILLVAR 'sparrslc',cumsptmp
	end
	cumsptmp += 1
	jump 'cumsptmploop'
!{Teeth Clean}
elseif cumspclnt = 2:
	if sparrloc = 12:
		KILLVAR 'spafinloc',cumsptmp
		KILLVAR 'sparrage',cumsptmp
		KILLVAR 'sparrloc',cumsptmp
		KILLVAR '$sparrnam',cumsptmp
		KILLVAR 'sparrppt',cumsptmp
		KILLVAR 'sparridt',cumsptmp
		KILLVAR 'sparrvol',cumsptmp
		KILLVAR 'sparrslc',cumsptmp
	end
	cumsptmp += 1
	jump 'cumsptmploop'
!{Internal Clean}
elseif cumspclnt = 3:
	if sparrloc = 0 or sparrloc = 3 or sparrloc = 17:
		KILLVAR 'spafinloc',cumsptmp
		KILLVAR 'sparrage',cumsptmp
		KILLVAR 'sparrloc',cumsptmp
		KILLVAR '$sparrnam',cumsptmp
		KILLVAR 'sparrppt',cumsptmp
		KILLVAR 'sparridt',cumsptmp
		KILLVAR 'sparrvol',cumsptmp
		KILLVAR 'sparrslc',cumsptmp
	end
	cumsptmp += 1
	jump 'cumsptmploop'
!{External Body Clean}
elseif cumspclnt = 4:
	if sparrloc = 1 or sparrloc = 4 or (sparrloc >= 8 and sparrloc <= 11) or (sparrloc >= 13 and sparrloc <= 16):
		KILLVAR 'spafinloc',cumsptmp
		KILLVAR 'sparrage',cumsptmp
		KILLVAR 'sparrloc',cumsptmp
		KILLVAR '$sparrnam',cumsptmp
		KILLVAR 'sparrppt',cumsptmp
		KILLVAR 'sparridt',cumsptmp
		KILLVAR 'sparrvol',cumsptmp
		KILLVAR 'sparrslc',cumsptmp
	end
	cumsptmp += 1
	jump 'cumsptmploop'
!{Hair Clean}
elseif cumspclnt = 5:
	if sparrloc = 16:
		KILLVAR 'spafinloc',cumsptmp
		KILLVAR 'sparrage',cumsptmp
		KILLVAR 'sparrloc',cumsptmp
		KILLVAR '$sparrnam',cumsptmp
		KILLVAR 'sparrppt',cumsptmp
		KILLVAR 'sparridt',cumsptmp
		KILLVAR 'sparrvol',cumsptmp
		KILLVAR 'sparrslc',cumsptmp
	end
	cumsptmp += 1
	jump 'cumsptmploop'
!{Clothes Clean}
elseif cumspclnt = 6:
	if sparrloc = 2 or (sparrloc >= 5 and sparrloc <= 7):
		KILLVAR 'spafinloc',cumsptmp
		KILLVAR 'sparrage',cumsptmp
		KILLVAR 'sparrloc',cumsptmp
		KILLVAR '$sparrnam',cumsptmp
		KILLVAR 'sparrppt',cumsptmp
		KILLVAR 'sparridt',cumsptmp
		KILLVAR 'sparrvol',cumsptmp
		KILLVAR 'sparrslc',cumsptmp
	end
	cumsptmp += 1
	jump 'cumsptmploop'
!{Enima}
elseif cumspclnt = 7:
	if sparrloc = 3:
		KILLVAR 'spafinloc',cumsptmp
		KILLVAR 'sparrage',cumsptmp
		KILLVAR 'sparrloc',cumsptmp
		KILLVAR '$sparrnam',cumsptmp
		KILLVAR 'sparrppt',cumsptmp
		KILLVAR 'sparridt',cumsptmp
		KILLVAR 'sparrvol',cumsptmp
		KILLVAR 'sparrslc',cumsptmp
	end
	cumsptmp += 1
	jump 'cumsptmploop'
!{Douche}
elseif cumspclnt = 8:
	if sparrloc = 0 or sparrloc >= 17:
		KILLVAR 'spafinloc',cumsptmp
		KILLVAR 'sparrage',cumsptmp
		KILLVAR 'sparrloc',cumsptmp
		KILLVAR '$sparrnam',cumsptmp
		KILLVAR 'sparrppt',cumsptmp
		KILLVAR 'sparridt',cumsptmp
		KILLVAR 'sparrvol',cumsptmp
		KILLVAR 'sparrslc',cumsptmp
	end
	cumsptmp += 1
	jump 'cumsptmploop'
!{Handwashing}
elseif cumspclnt = 9:
	if sparrloc = 13:
		KILLVAR 'spafinloc',cumsptmp
		KILLVAR 'sparrage',cumsptmp
		KILLVAR 'sparrloc',cumsptmp
		KILLVAR '$sparrnam',cumsptmp
		KILLVAR 'sparrppt',cumsptmp
		KILLVAR 'sparridt',cumsptmp
		KILLVAR 'sparrvol',cumsptmp
		KILLVAR 'sparrslc',cumsptmp
	end
	cumsptmp += 1
	jump 'cumsptmploop'
!{Full External Clean}
elseif cumspclnt = 10:
	if sparrloc = 1 or sparrloc = 2 or (sparrloc >= 4 and sparrloc <= 11) or (sparrloc >= 13 and sparrloc <= 16):
		KILLVAR 'spafinloc',cumsptmp
		KILLVAR 'sparrage',cumsptmp
		KILLVAR 'sparrloc',cumsptmp
		KILLVAR '$sparrnam',cumsptmp
		KILLVAR 'sparrppt',cumsptmp
		KILLVAR 'sparridt',cumsptmp
		KILLVAR 'sparrvol',cumsptmp
		KILLVAR 'sparrslc',cumsptmp
	end
	cumsptmp += 1
	jump 'cumsptmploop'
!{Face Cleanup}
elseif cumspclnt = 11:
	if sparrloc = 11:
		KILLVAR 'spafinloc',cumsptmp
		KILLVAR 'sparrage',cumsptmp
		KILLVAR 'sparrloc',cumsptmp
		KILLVAR '$sparrnam',cumsptmp
		KILLVAR 'sparrppt',cumsptmp
		KILLVAR 'sparridt',cumsptmp
		KILLVAR 'sparrvol',cumsptmp
		KILLVAR 'sparrslc',cumsptmp
	end
	cumsptmp += 1
	jump 'cumsptmploop'
!{Face and Hair Cleanup}
elseif cumspclnt = 12:
	if sparrloc = 11 or sparrloc = 16:
		KILLVAR 'spafinloc',cumsptmp
		KILLVAR 'sparrage',cumsptmp
		KILLVAR 'sparrloc',cumsptmp
		KILLVAR '$sparrnam',cumsptmp
		KILLVAR 'sparrppt',cumsptmp
		KILLVAR 'sparridt',cumsptmp
		KILLVAR 'sparrvol',cumsptmp
		KILLVAR 'sparrslc',cumsptmp
	end
	cumsptmp += 1
	jump 'cumsptmploop'
!{Head clean}
elseif cumspclnt = 13:
	if sparrloc = 11 or sparrloc = 12 or sparrloc = 16:
		KILLVAR 'spafinloc',cumsptmp
		KILLVAR 'sparrage',cumsptmp
		KILLVAR 'sparrloc',cumsptmp
		KILLVAR '$sparrnam',cumsptmp
		KILLVAR 'sparrppt',cumsptmp
		KILLVAR 'sparridt',cumsptmp
		KILLVAR 'sparrvol',cumsptmp
		KILLVAR 'sparrslc',cumsptmp
	end
	cumsptmp += 1
	jump 'cumsptmploop'
!{Full Clean}
elseif cumspclnt = 20:
	KILLVAR 'spafinloc'
	KILLVAR 'sparrage'
	KILLVAR 'sparrloc'
	KILLVAR '$sparrnam'
	KILLVAR 'sparrppt'
	KILLVAR 'sparridt'
	KILLVAR 'sparrvol'
	KILLVAR 'sparrslc'
	cumsptmp += 1
	jump 'cumsptmploop'
else
	cumsptmp = 0
	cumspclnt = 0
end
spdirty = 0
spdirtyi = 0
spdirtyn = 0
spdirtyv = 0
spdirtyb = 0
spdirtyc = 0

