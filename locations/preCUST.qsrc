		EggRH = 0
		FocH = 0
		unfertegg = 0
		Ovulate = 0
		daylastperiod -= 10+(ferteggage/24)
	end
	
	if daylastperiod + 4 > daystart:
		lastmens = daylastperiod - 20
	else
		lastmens = daylastperiod+4
	end

	if difficulty = 4:
		cyccustom = 0
	else
		cyccustom = 1
	end
	
	pcs_horny = 0
	killvar 'opPRE'
	killvar 'tgs_skipinto'
	gs 'stat'
	showstat 0
	showobjs 0
	if $ARGS[1] = 'NT':
		gt 'preNT'
	elseif $ARGS[1] = 'TGS':
		gt 'preTG', 'sleepPRE'
	elseif $ARGS[1] = 'CSB':
		gt 'preCSB', 'intro_0'
	else
		gt 'preALT', 'One'
	end
end

