# din_npc

! enables going home to current house
! 1 = parents house
! 2 = Grandparents House
! 3 = St. Petersburg Apt

$go_home = {
		if curr_home = 1:
		gt 'pavResidential'
		elseif curr_home = 2:
		gt 'gadukino'
		elseif curr_home = 3:
		gt 'street'
		end
}

$go_straight_home = {
		if curr_home = 1:
		gt 'korrPar'
		elseif curr_home = 2:
		gt 'gaddvor'
		elseif curr_home = 3:
		gt 'korr'
		end
}
