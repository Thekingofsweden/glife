$prezik = {
	if prezik > 0:
		if badprezik > 0:
			if rand(1,prezik) <= badprezik: badprezik -= 1 & noprotect = 1
		else
			noprotect = 0
		end
		prezik -= 1
	elseif prezik < 0:
		prezik = 0
	end
}

