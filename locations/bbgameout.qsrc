						act 'Drive in (<<will_cost>> Willpower)':
							*clr & cla
							grupvalue[2] += 1
							pcs_mood += 40
							bkbll_exp += rand(0,2)
							gs 'sweat', 'add', 40
							gs 'willpower', 'pay', 'self'
							gs 'stat'
