# portnoi

$batch_sell = {
	poshvalldrop += 1
	dynamic 'RESULT = <<$ARGS[0]>>H[<<ARGS[1]>>]'
	if RESULT > 100:
		dynamic '<<$ARGS[0]>>[<<ARGS[1]>>] = 0'
		dynamic '<<$ARGS[0]>>S[<<ARGS[1]>>] = 0'
		money += 750
		selltot += 750
	elseif RESULT > 45:
		dynamic '<<$ARGS[0]>>[<<ARGS[1]>>] = 0'
		dynamic '<<$ARGS[0]>>S[<<ARGS[1]>>] = 0'
		money += 500
		selltot += 500
	elseif RESULT > 30:
		dynamic '<<$ARGS[0]>>[<<ARGS[1]>>] = 0'
		dynamic '<<$ARGS[0]>>S[<<ARGS[1]>>] = 0'
		money += 300
		selltot += 300
	elseif RESULT > 0:
		dynamic '<<$ARGS[0]>>[<<ARGS[1]>>] = 0'
		dynamic '<<$ARGS[0]>>S[<<ARGS[1]>>] = 0'
		money += 100
		selltot += 100
	else
		dynamic '<<$ARGS[0]>>[<<ARGS[1]>>] = 0'
		dynamic '<<$ARGS[0]>>S[<<ARGS[1]>>] = 0'
	end
}


