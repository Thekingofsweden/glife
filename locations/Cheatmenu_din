# Cheatmenu_din
!All the active dynamics for the cheat menu are here

$din_cheat_statusbars = {
	if cheatStatusBars = 0:
		cheatStatusBars = 1
		dynamic $din_cheat_menu_view
		exit
	elseif cheatStatusBars = 1:
		cheatStatusBars = 0
		dynamic $din_cheat_menu_view
	end
}

$din_cheat_work = {
	if cheatWork = 0:
		cheatWork = 1
		dynamic $din_cheat_menu_permanent
		exit
	elseif cheatWork = 1:
		cheatWork = 0
		dynamic $din_cheat_menu_permanent
	end
}

$din_cheat_hapri = {
	if cheatHapri = 0:
