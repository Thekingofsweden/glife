		'<center><img src="images/zsoft/pod_ezd/elevator_ob'+rand(1,2)+'.jpg"></center>'
	act 'Continue': gt'pod_ezd','lift'
end
!locked ads
if $ARGS[0] = 'pod_objava':
		cls
		minut +=1
		gs'stat'
		'<center><img src="images/zsoft/pod_ezd/pod_ob'+rand(1,2)+'.jpg"></center>'
	act 'Continue': gt'pod_ezd','etaj_1'
end

