		internet = 100000
		act 'Browser':gt 'Komp', 'brows'
	else
		if internet > 0:
			'You have <<internet>> hours of Internet access remaining.'

			act 'Browser':gt 'Komp', 'brows'
		end

		if karta >= 10:
			'You can pay online through your bank account. 1 hour of internet access costs 10 rubles. You have <<karta>>rubles in your account.'

			act 'Buy more account access':
				cla
				intinp = input ("How many hours of internet access do you wish to purchase? (1hr = 10 rubles)")

				if intinp <= 0 or intinp * 10 > karta:
					'Invalid amount.'
				else
					karta -= intinp * 10
					internet += intinp
					'You paid for <<inttime>> hours of Internet access, you have <<internet>> hours of the Internet access remianing. <<intinp>> rubles were removed from your account.'
				end

				act 'Exit payment system':gt 'Komp', 'start'
			end
		end
	end

	act 'Shut your computer down':
		if $LOC = 'shulga_room':
			internet = internetS
		end
		cla
		gt 'Komp', 'fin'
	end
end

